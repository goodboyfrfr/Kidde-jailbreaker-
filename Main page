    <script>
        function runHack() {
            let outputBox = document.getElementById("output");
            outputBox.innerHTML = ""; 
            for (let i = 1; i <= 5; i++) {
                outputBox.innerHTML += `Hacker thing ${i}\n`;
            }
            outputBox.innerHTML += "Boom! Did the magic!";
        }
